        //作为回调函数使用,弹出一个toast告诉回调成功了。
        function Callback(){
            this.alert = function() {
                if(arguments.length==0) {
                    window.open('jsbridge://JSInteract/showToast/1/触发回调了');
                }
                else if(arguments.length==1){
                    var str = encodeURIComponent(arguments[0]);
                    window.open('jsbridge://JSInteract/showToast/1/自定义字段:' + str);
                }
            }
        }
        var callback = new Callback();

        function JsBridge() {
            this.callback = function(sn, res) {
                if (res.result != 'undefined') {
                    var str = encodeURIComponent(res.result);
                    window.open('jsbridge://JSInteract/showToast/1/返回的结果：' + str);
                }
            }
        }
        var JsBridge = new JsBridge();


